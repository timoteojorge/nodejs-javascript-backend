<html>
	<body>
		<table border='1px'>
			<tr>
				<td>Id</td>
				<td>Nome</td>
				<td>Descrição</td>
				<td>Preço</td>
			</tr>
			<% if(lista.length > 0 )
			for(var i=0;i<lista.length;i++) {%>
			<tr>
				<td><%=lista[i].id%></td>
				<td><%=lista[i].titulo%></td>
				<td><%=lista[i].descricao%></td>
				<td><%=lista[i].preco%></td>
			</tr>
			<%}%>
		</table>
	</body>
</html>